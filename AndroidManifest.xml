<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adsg0186.blobmergency"
    android:versionCode="1.1"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="16" />
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.adsg0186.shapemergency.MainActivity"
            android:label="@string/app_name" 
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.adsg0186.shapemergency.GameActivity"
            android:label="@string/app_name" 
            android:screenOrientation="portrait" >
            <intent-filter>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="com.adsg0186.shapemergency.HighScoreView" android:screenOrientation="portrait"></activity>
        <activity android:name="com.adsg0186.shapemergency.SettingsView" android:label="@string/app_name" android:screenOrientation="portrait"></activity>
        <activity android:name="com.adsg0186.shapemergency.HelpView" android:label="@string/app_name" android:screenOrientation="portrait"></activity>

    </application>

</manifest>
